   "metadata": {},
   "source": [
    "#### 웹 서버 생성 (nginx)\n",
    "\n",
    "1. sudo su - root\n",
    "2. yum update\n",
    "3. yum install https://dl.fedoraproject.org/pub/epel/epel-release-latest-7.noarch.rpm\n",
    "4. yum -y install nginx\n",
    "5. service nginx restart\n",
    "6. vi /usr/share/nginx/html/index.html"
   ]
  },
